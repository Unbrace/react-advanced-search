{
  "name": "react-advanced-search",
  "version": "0.0.0",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unbrace/react-advanced-search.git"
  },
  "files": [
    "lib"
  ],
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-flow-react-proptypes": "^0.21.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "flow-bin": "^0.43.0",
    "flow-copy-source": "^1.1.0",
    "husky": "^0.13.3",
    "lint-staged": "^3.4.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "prettier": "^0.22.0",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-dom": "^15.4.2",
    "styled-components": "^1.4.4"
  },
  "peerDependencies": {
    "lodash": "^4.17.4",
    "react": "^15.4.2",
    "react-addons-update": "^15.4.2",
    "react-dom": "^15.4.2",
    "styled-components": "^1.4.4"
  },
  "scripts": {
    "build": "npm run build:lib && npm run build:flow",
    "prebuild:lib": "rm -rf lib/*",
    "build:lib": "babel src --ignore src/stories/* --out-dir lib",
    "build:flow": "flow-copy-source -v -i '{**/test/*.js,**/*.test.js,**/stories/*}' src lib",
    "prepublish": "npm run build",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "lint-staged": {
    "*.js": [
      "prettier --trailing-comma es5 --single-quote --print-width 100 --write",
      "git add"
    ]
  },
  "dependencies": {
    "react-select": "^1.0.0-rc.3"
  }
}
